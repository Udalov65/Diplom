<Window x:Class="Diplom.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Diplom"
        mc:Ignorable="d"
        Title="" Height="746" Width="1200" ResizeMode="NoResize" WindowStartupLocation="CenterScreen"
    Icon="/Image/logo.png">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="116*"/>
            <RowDefinition Height="403*"/>
        </Grid.RowDefinitions>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Width="440">
            <Image Source="Image/logo.png" Height="128" Width="122"/>
            <Label Content="ООО &quot;Сиб-строй&quot;" FontSize="36" FontWeight="Bold"/>
        </StackPanel>
        <Frame x:Name="FrmMain" NavigationUIVisibility="Hidden" Grid.Row="1" Navigated="FrmMain_Navigated"/>
    </Grid>
</Window>
